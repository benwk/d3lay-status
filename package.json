    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^7.0.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-v8": "^0.34.3",
    "cypress": "^13.1.0",
    "eslint": "^8.48.0",
    "eslint-plugin-astro": "^0.29.0",
    "release-it": "^16.1.5",
    "vitest": "^0.34.3",
    "wrangler": "^3.19.0"
